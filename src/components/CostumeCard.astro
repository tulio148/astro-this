---
// Astro props
import { Image } from "astro:assets";
import CostumeCardReact from "./react/CostumeCard";

interface Props {
  title: string;
  description: string;
  color: string;
  size: string;
  material: string;
  price: string;
  image: any;
}

const { title, description, color, size, material, price, image } = Astro.props;
---

<CostumeCardReact
  client:load
  title={title}
  description={description}
  color={color}
  size={size}
  material={material}
  price={price}
  image={image}
/>
